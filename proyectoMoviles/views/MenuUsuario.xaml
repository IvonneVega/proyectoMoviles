<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="proyectoMoviles.views.MenuUsuario" BackgroundImageSource="menu4.png">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <StackLayout.Padding>
                <OnPlatform x:TypeArguments="Thickness"
                  iOS="40,40,40,40"
                  Android="40,40,40,40" />
            </StackLayout.Padding>
            <Button x:Name="btnPerfil" Text="Perfil" HeightRequest="100" WidthRequest="100" Clicked="btnPerfil_Clicked"/>
            <Button x:Name="btnSupereroes" Text="Superheroes" HeightRequest="100" WidthRequest="100" Clicked="btnSupereroes_Clicked"/>
        </StackLayout>


    </ContentPage.Content>
</ContentPage>